{
    "development": {
        "css": ["http://staticdev.demo.com/css/lib/normalize/3.0.2/normalize.css", "http://staticdev.demo.com/css//app/common.css"],
        "js": ["http://staticdev.demo.com/js/lib/requirejs/2.1.16/require.js"],
        "requirejsConfig": {
            "baseUrl": "http://staticdev.demo.com/js",
            "paths": {
                "backbone": "lib/backbone/1.1.2/backbone",
                "jquery": "lib/jquery/1.7.2/jquery",
                "swiper": "lib/swiper/3.0.4/swiper.jquery",
                "underscore": "lib/underscore/1.8.2/underscore"
            },
            "shim": {
                "backbone": {
                    //These script dependencies should be loaded before loading
                    //backbone.js
                    "deps": ["underscore", "jquery"],
                    //Once loaded, use the global 'Backbone' as the
                    //module value.
                    "exports": "Backbone"
                },
                "underscore": {
                    "exports": "_"
                },
            }
        },
        "requirejsMain": "app/view/index"
    },
    "production": {
        "css": ["http://static.demo.com/css/app-index.css"],
        "js": ["http://static.demo.com/js/lib/requirejs/2.1.16/require.min.js"],
        "requirejsConfig": {
            "baseUrl": "http://static.demo.com/js"
        },
        "requirejsMain": "app-index"
    }
}